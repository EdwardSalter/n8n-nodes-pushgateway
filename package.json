{
	"name": "n8n-nodes-pushgateway",
	"version": "0.1.0",
	"description": "A custom node for n8n allowing integration into Prometheus Pushgateway",
	"license": "MIT",
	"homepage": "",
	"keywords": [
		"n8n-community-node-package"
	],
	"author": {
		"name": "Edward Salter",
		"email": "edward.salter@live.co.uk"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/EdwardSalter/n8n-nodes-pushgateway.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease",
		"start": "n8n start"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/PushgatewayApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Pushgateway/Pushgateway.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2",
		"vitest": "^4.0.16"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
